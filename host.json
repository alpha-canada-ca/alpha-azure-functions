{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true
      }
    },
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "Host.Triggers": "Warning",
      "Host.Results": "Warning",
      "Host.Aggregator": "Warning"
    },
    "fileLoggingMode": "never"
  }
}
